@model PRIS.Web.Models.CityCreateModel
@{
    ViewData["Title"] = "CreateNewCity";
}

<h1>Sukurti naują miestą</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateNewCity">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CityName" class="control-label">Miesto pavadinimas</label>
                <input asp-for="CityName" class="form-control" />
                <span asp-validation-for="CityName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Sukurti" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Grįžti</a>
</div>

